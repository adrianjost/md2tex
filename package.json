{
	"name": "@adrianjost/md2tex",
	"version": "1.1.1",
	"description": "bachelor thesis basic Markdown to Latex converter",
	"main": "dist/md2tex.js",
	"bin": {
		"md2tex": "dist/md2tex.file.js"
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"convert": "parcel watch ./md2tex.file.js",
		"test": "jest",
		"test:update": "jest -u",
		"test:watch": "yarn test --watchAll",
		"build:converter": "parcel build md2tex.js",
		"build:fileConverter": "parcel build md2tex.file.js",
		"build": "run-p build:*"
	},
	"keywords": [
		"Markdown",
		"Latex",
		"Converter",
		"Thesis"
	],
	"contributors": [
		{
			"name": "Adrian Jost",
			"email": "me@adrianjost.dev",
			"url": "https://adrianjost.dev"
		}
	],
	"license": "MIT",
	"jest": {
		"coverageDirectory": "./coverage/",
		"collectCoverage": true
	},
	"devDependencies": {
		"@babel/core": "^7.5.4",
		"@babel/preset-env": "^7.5.4",
		"babel-jest": "^24.8.0",
		"dedent-tabs": "^0.8.0",
		"jest": "^24.8.0",
		"nodemon": "^1.19.1",
		"npm-run-all": "^4.1.5",
		"parcel-bundler": "^1.12.3",
		"parcel-plugin-shebang": "^1.2.7"
	},
	"engines": {
		"node": ">=8"
	}
}
